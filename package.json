{
  "name": "mat-for-js",
  "version": "1.1.0",
  "description": "Converts Matlab Level 5 MAT-files to JavaSript objects",
  "main": "lib/index.js",
  "type": "module",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "webpack -c ./webpack.dev.config.cjs; webpack -c ./webpack.prod.config.cjs",
